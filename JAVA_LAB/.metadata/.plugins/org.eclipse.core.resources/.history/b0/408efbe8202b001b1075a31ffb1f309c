
public class Number {

	public static void main(String[] args) {
		//operator
		System.out.println(6 + 2);
		System.out.println(6 - 2);
		System.out.println(6 * 2);
		System.out.println(6 / 2);
		System.out.println(6 % 2);
		
		System.out.println(Math.PI);
		System.out.println(Math.do(Math.PI));
	}

}
