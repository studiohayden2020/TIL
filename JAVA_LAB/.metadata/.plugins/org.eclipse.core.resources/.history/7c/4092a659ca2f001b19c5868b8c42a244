package finalex;

public class UsingDefine {
	public static void main(String[] args) {
		System.out.println(Define.GOOD_MORNING);
		System.out.println("the MIN value is " + Define.MIN);
		System.out.println("the MAX value is " + Define.MAX);
		System.out.println("the MATA code value is" + Define.MATH);
		System.out.println("the ENG code value is" + Define.ENG);
		
	}

}
